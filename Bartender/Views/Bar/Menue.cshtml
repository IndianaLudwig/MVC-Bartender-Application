@model IEnumerable<BarMenueModel>
@{
    ViewData["Title"] = "Menue Page";
}

<div class="text-center">
    <h1 class="display-4">Menue</h1>
    <table class ="table table-striped table-bordered"" style="width:100%">
        <thead>
            <tr>
                <th>Name</th>
                <th>Description</th>
                <th>Price</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var drink in Model)
            {
                <tr>
                    <td>@.</td>
                    <td></td>
                    <td></td>
                </tr>
            }
        </tbody>
     </table>
</div>